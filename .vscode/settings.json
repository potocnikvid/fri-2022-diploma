{
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/__pycache__": true
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/.hg/store/**": true,
        "**/wandb/**": true,
        "**/.cache/**": true,
        "**/.tmp/**": true,
        "**/.ipynb_checkpoints/**": true,
        "**/submodules/**": true,
        "**/__pycache__/**": true,
    },
    "python.envFile": "${workspaceFolder}/dev.env",
    "python.terminal.activateEnvironment":true,
    "terminal.integrated.env.linux": {
        "PYTHONPATH": "${workspaceFolder}/src:${workspaceFolder}/submodules/stylegan2-ada-pytorch"
    },
    "jupyter.notebookFileRoot": "${workspaceFolder}",
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#8fa7ed",
        "activityBar.activeBorder": "#dc2352",
        "activityBar.background": "#8fa7ed",
        "activityBar.foreground": "#15202b",
        "activityBar.inactiveForeground": "#15202b99",
        "activityBarBadge.background": "#dc2352",
        "activityBarBadge.foreground": "#e7e7e7",
        "statusBar.background": "#6384e6",
        "statusBar.foreground": "#15202b",
        "statusBarItem.hoverBackground": "#3761df",
        "titleBar.activeBackground": "#6384e6",
        "titleBar.activeForeground": "#15202b",
        "titleBar.inactiveBackground": "#6384e699",
        "titleBar.inactiveForeground": "#15202b99"
    },
    "peacock.remoteColor": "#6384e6"
}